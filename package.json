		"zone.js": "^0.8.2"
	},
	"devDependencies": {
		"ng-packagr": "^3.0.3",
		"@angular/core": "^6.0.0",
		"@angular/compiler": "^6.0.0",
		"@angular/compiler-cli": "^6.0.0",
		"tsickle": "^0.27.2",
		"typescript": "2.7.x",
		"tslib": "^1.9.0"
